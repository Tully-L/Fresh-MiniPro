{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "sku": {
      "type": "string",
      "required": true,
      "unique": true,
      "default": "VG-001-J"
    },
    "productName": {
      "type": "string",
      "required": true
    },
    "alias": {
      "type": "string"
    },
    "productStatus": {
      "type": "enumeration",
      "enum": [
        "online",
        "offline"
      ],
      "default": "online",
      "required": true
    },
    "unit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::unit.unit",
      "inversedBy": "products"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "order_item": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order-item.order-item",
      "inversedBy": "products"
    }
  }
}
