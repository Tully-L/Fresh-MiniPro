{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "address": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::address.address",
      "inversedBy": "orders"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "completed",
        "canceled"
      ],
      "default": "pending",
      "required": true
    },
    "orderTime": {
      "type": "datetime",
      "required": true
    },
    "orderNote": {
      "type": "text"
    },
    "printStatus": {
      "type": "enumeration",
      "enum": [
        "notPrinted",
        "printed"
      ],
      "default": "notPrinted",
      "required": true
    },
    "printTime": {
      "type": "datetime",
      "required": true
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "order"
    }
  }
}
